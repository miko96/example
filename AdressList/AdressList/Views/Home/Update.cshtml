@using AdressList.Models
@model AdressNote

@{
    ViewBag.Title = "Update Note";
}
<div id="update" class="section panel panel-primary">
    <div class="panel-heading">
        Update
    </div>
    <div class="panel-body">
        @using (Html.BeginForm())
        {
            <div class="form-group">
                <label>Peson Name</label>
                @Html.TextBoxFor(m => m.PersonName, new { @class = "form-control" })
            </div>
            <div class="form-group">
                <label>Adress</label>
                @Html.TextBoxFor(m => m.Adress, new { @class = "form-control" })
            </div>
            <button type="submit" class="btn btn-primary">Save</button>
        }
    </div>
</div>